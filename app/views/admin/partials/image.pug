#image.mb-3
	#image-preview
		img.img-fluid(src=data.imageFormats.large alt=data.stagename)
	#formupload.d-none(style='max-width:400px')
		.enableBorder
			.embed-responsive.embed-responsive-1by1.mb-0
				.embed-responsive-item
					.attachment-dropzone(aria-disabled='false' style='position: relative;height:100%')
						.labelFieldUpload
							.h1
								i.far.fa-arrow-alt-circle-down
							span=__("Drop file here")
							div=__("OR")
							.btn.btn-primary=__("BROWSE")
